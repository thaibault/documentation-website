{
    "description": "documentationWebsite",
    "dependencies": {
        "bootstrap": "^3.3.6",
        "jquery": "2.2.2"
    },
    "devDependencies": {
        "webOptimizer": "git+ssh://git@github.com/thaibault/webOptimizer.git",
        "jQuery-website": "git+ssh://git@github.com/thaibault/jQuery-website.git",
        "genericStyle": "git+ssh://git@github.com/thaibault/genericStyle.git"
    },
    "license": "CC-BY-3.0",
    "name": "website",
    "version": "1.0.0",
    "keywords": ["documentation", "website", "project", "homepage", "page"],
    "files": ["source", "generateDummyDocumentation.sh"],
    "main": "source/coffeeScript/main.coffee",
    "repository": {
        "type": "git",
        "url": "git@github.io:thaibault/documentationWebsite.git"
    },
    "webOptimizer": {
        "debug": true,
        "moduleAliases": {
            "bootstrap": "bootstrap/dist/css/bootstrap"
        },
        "internalInjects": [
            "less/main",
            "javaScript/main"
        ],
        "externalInjects": ["babel-polyfill"],
        "preprocessor": {"modernJavaScript": {"plugins": [
            "transform-decorators-legacy",
            "transform-class-properties",
            "transform-flow-strip-types"
        ]}}
    },
    "scripts": {
        "lint": "webOptimizer lint",
        "clear": "webOptimizer clear",
        "build": "webOptimizer build",
        "server": "webOptimizer server",
        "start": "webOptimizer server"
    }
}
