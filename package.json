{
    "name": "website",
    "version": "1.0.0",
    "keywords": ["documentation", "website", "project", "homepage", "page"],
    "description": "documentationWebsite",
    "license": "CC-BY-3.0",
    "files": ["source", "dummyDocumentation.pug"],
    "main": "build/index.html",
    "repository": {
        "type": "git",
        "url": "git@github.io:thaibault/documentationWebsite.git"
    },
    "dependencies": {
        "webOptimizer": "git+ssh://git@github.com/thaibault/webOptimizer.git",
        "jQuery-website": "git+ssh://git@github.com/thaibault/jQuery-website.git",
        "legalNotes": "git+ssh://git@github.com/thaibault/legalNotes.git"
    },
    "webOptimizer": {
        "buildDefinition": {
            "GOOGLE_TRACKING_CODE": "''",
            "LANGUAGES": {
                "__execute__": "`['${self.module.preprocessor.pug.locals.LANGUAGES.join(\"', '\")}']`"
            }
        },
        "injection": {"internal": {
            "__execute__": "{vendor: self.debug ? ['jQuery-website'] : [], index: ['less/index', 'index'].concat(self.debug ? [] : 'babel-polyfill')}"
        }},
        "library": false,
        "module": {
            "aliases": {
                "bootstrap.less$": "bootstrap/less/bootstrap",
                "jQuery-scrollTo$": "jquery.scrollto/jquery.scrollTo",
                "jQuery-spin$": "spin.js/jquery.spin",
                "genericStyle.less$": "genericStyle/index.less",
                "jQuery-website.less$": "jQuery-website/index.less"
            },
            "preprocessor": {"pug": {"locals": {
                "CONTENT_FILE_PATH": "dummyDocumentation.pug",
                "LANGUAGES": ["deDE", "enUS", "frFR"],
                "NAME": "productName",
                "RENDER_CONTENT": true,
                "TAGLINE": "tagline",
                "SOURCE_URL": "https://github.com/thaibault/documentationWebsite",
                "URL": "https://github.com/thaibault/documentationWebsite"
            }}},
            "skipParseRegularExpression": {
                "__execute__": "/\\/(?:(?:jquery)|(?:module)|(?:spin))\\.js(?:\\?.+|$)/"
            }
        },
        "path": {"asset": {"javaScript": "./"}}
    },
    "scripts": {
        "build": "webOptimizer build",
        "buildWithStats": "webOptimizer build --profile --json >/tmp/stat.json && echo 'Results successfully written to \"/tmp/stat.json\".'",
        "clear": "webOptimizer clear",
        "document": "webOptimizer document",
        "lint": "webOptimizer typeCheck && webOptimizer lint",
        "postinstall": "npm run build",
        "preinstall": "mkdir --parents build && touch build/index.html",
        "serve": "webOptimizer serve",
        "start": "npm run serve",
        "test": "webOptimizer test",
        "testInBrowser": "webOptimizer testInBrowser",
        "watch": "webOptimizer build --watch"
    }
}
